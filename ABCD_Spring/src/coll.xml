<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="data" class="com.collection.DataStorer" scope="prototype">
		<property name="list">
			<list>
				<value>"Suraj"</value>
				<value>"Virat"</value>
				<value>"Rahul"</value>

			</list>
		</property>
		<constructor-arg>
			<set>
				<value>"Mahi"</value>
				<value>"Aman"</value>
				<value>"Rahul"</value>

			</set>
		</constructor-arg>
		<property name="map">
			<map>
				<entry key ="1" value="Sudhir"/>
				<entry key ="2" value="mahesh"/>
			</map>
		</property>
	</bean>
</beans>
	