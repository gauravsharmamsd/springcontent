<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <servlet>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>com.rkit.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>lifeCycle</servlet-name>
    <servlet-class>com.rkit.LifeCycleDemoServlet</servlet-class>
    <init-param>
      <param-name>userId</param-name>
      <param-value>user1</param-value>
    </init-param>
    <init-param>
      <param-name>passwd</param-name>
      <param-value>password1</param-value>
    </init-param>
    <init-param>
      <param-name>email</param-name>
      <param-value>vivek@ramkrishnait.com</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>lifeCycle</servlet-name>
    <url-pattern>/life</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AnotherServlet</servlet-name>
    <servlet-class>com.rkit.AnotherServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AnotherServlet</servlet-name>
    <url-pattern>/another</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Page1Servlet</display-name>
    <servlet-name>Page1Servlet</servlet-name>
    <servlet-class>com.rkit.Page1Servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Page1Servlet</servlet-name>
    <url-pattern>/page1</url-pattern>
  </servlet-mapping>
</web-app>